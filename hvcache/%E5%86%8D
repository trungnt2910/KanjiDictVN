41]],[[367,575],[390,564],[637,613],[656,613],[679,603],[702,576],[711,193],[706,78],[693,48],[651,56],[582,83]],[[479,722],[512,699],[510,349],[494,329]],[[408,438],[459,433],[573,461],[617,462]],[[100,278],[162,261],[430,298],[849,334],[933,306]]]}">
